{
  "name": "fastpro-test-dev-pleno",
  "version": "1.0.0",
  "description": "fastpro test dev pleno",
  "type": "commonjs",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "~27.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "~16.11",
    "@types/node-fetch": "^2.6.1",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "~5.14",
    "@typescript-eslint/parser": "~5.14",
    "eslint": "~8.11",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~26.1",
    "jest": "~27.5",
    "prettier": "~2.5",
    "rimraf": "~3.0",
    "ts-jest": "~27.1",
    "tsutils": "~3.21",
    "typescript": "~4.6"
  },
  "scripts": {
    "start": "tsnd -r tsconfig-paths/register --transpile-only --ignore-watch node_modules ./src/presentation/infrastructure/http/server.ts",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -n",
    "typeorm:migration:run": "npm run typeorm -- migration:run"
  },
  "author": "Ives Samuel <ivessamuel64@gmail.com>",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "celebrate": "^13.0.4",
    "class-transformer": "^0.3.1",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "ioredis": "^4.19.2",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.7",
    "ora": "^5.1.0",
    "pg": "^8.4.2",
    "rate-limiter-flexible": "^2.1.13",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.0",
    "tsyringe": "^4.4.0",
    "typeorm": "^0.2.29",
    "uuidv4": "^6.2.5"
  },
  "volta": {
    "node": "16.13.0"
  }
}
